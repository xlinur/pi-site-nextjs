{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^20",
    "npm": "^10"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint . --error-on-unmatched-pattern --fix",
    "commit": "git-cz",
    "format": "prettier --write",
    "test": "vitest watch --sequence.concurrent=true",
    "test:unit": "vitest --sequence.concurrent=true",
    "test:no-watch": "vitest run --sequence.concurrent=true",
    "test:coverage": "vitest run --coverage --sequence.concurrent=true"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.2.0",
    "prettier": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitest/coverage-v8": "^1.6.0",
    "c8": "^7.12.0",
    "typescript": "^5",
    "vitest": "^1.6.0",
    "@mantine/form": "^7.12.2",
    "@sendgrid/mail": "^8.1.3",
    "@tsparticles/engine": "^3.5.0",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.5.0",
    "clsx": "^2.1.1",
    "qs": "^6.13.0",
    "react-google-recaptcha-v3": "^1.10.1",
    "sass": "^1.77.8",
    "swiper": "^11.1.10"
  }
}
